datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  binaryTargets   = ["native", "windows"]
}

model User {
  id        String   @id
  email     String   @unique
  name      String?
  password  String?        
  imageUrl  String?        
  createdAt DateTime @default(now())
}
